{
  "name": "mobidziennik-sdk",
  "version": "1.2.0",
  "description": "TypeScript web scraping SDK for Polish e-gradebook MobiDziennik",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "test": "echo Test!",
    "build": "tsc",
    "format": "prettier --write src/",
    "lint": "eslint src/",
    "prepare": "pnpm build",
    "prepublishOnly": "pnpm test && pnpm lint",
    "preversion": "pnpm lint",
    "version": "pnpm format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Norbiros/MobidziennikSDK.git"
  },
  "keywords": [
    "mobidziennik",
    "api"
  ],
  "license": "GPL",
  "contributors": [
    "Norbiros"
  ],
  "devDependencies": {
    "@types/node": "^22.9.3",
    "@eslint/js": "^9.15.0",
    "@types/cheerio": "^0.22.32",
    "@types/tough-cookie": "^4.0.2",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.12.0",
    "prettier": "^3.3.3",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.15.0"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "axios-cookiejar-support": "^5.0.3",
    "cheerio": "^1.0.0",
    "tough-cookie": "^5.0.0"
  },
  "packageManager": "pnpm@9.12.1"
}
